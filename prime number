package sep22.week1.day2;

public class PrimeNumber {

	public static void main(String[] args) {
		int input=89;
		boolean flag=false;
		for (int i=2;i<=input/2;i++) {
			flag=true;
			break;
		}

	
		if (flag==false)
	
	 System.out.println("Prime Number");
	
	else
	System.out.println("Not a Prime Number");
}
}
