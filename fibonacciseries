
public class Fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=8;
		int firstNum=0;
		int secNum=1;
		int sum=1;
		System.out.println(firstNum);
		for (int i=1;i<=num; i++) 
		{
			sum=firstNum+secNum;
			firstNum=secNum;
			secNum=sum;
			System.out.println(sum);
		}
	}

}
